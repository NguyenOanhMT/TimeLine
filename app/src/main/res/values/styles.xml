<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#157ccc</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="TextStyle">
        <item name="android:textSize">@dimen/sp17</item>
        <item name="android:textColor">@color/colorSlate_grey</item>
    </style>

    <style name="TextStyle9">
        <item name="android:textSize">28sp</item>
        <item name="android:lineSpacingExtra">-16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextStyle10">
        <item name="android:textSize">16.5sp</item>
        <item name="android:lineSpacingExtra">-10sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle11">
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra">-4sp</item>
        <item name="android:textColor">@color/colorBrownish_grey</item>
    </style>


</resources>
